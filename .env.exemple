# Chave secreta para JWT ou outra criptografia
# Secret key for JWT or other encryption
SECRET_KEY=<your_secret_key_here>

# Usuário do banco de dados
# Database user
DB_USER=<your_database_user>

# Senha do banco de dados
# Database password
DB_PASSWORD=<your_database_password>

# Nome do banco de dados
# Database name
DB_NAME=<your_database_name>

# Endereço do host do banco de dados (ex: localhost ou IP)
# Database host (e.g., localhost or IP address)
DB_HOST=<your_database_host>

# Porta do banco de dados (ex: 3306 para MySQL)
# Database port (e.g., 3306 for MySQL)
DB_PORT=<your_database_port>